kind: Secret
apiVersion: v1
metadata:
  name: llama-stack-secret
  labels:
    app.kubernetes.io/instance: llama-stack-secret
data:
  # Base64 encoded tokens - replace with your actual tokens
  deepseek-token: YOUR_DEEPSEEK_TOKEN_BASE64
  granite-token: YOUR_GRANITE_TOKEN_BASE64
  llama-token: YOUR_LLAMA_TOKEN_BASE64
  llama17b-token: YOUR_LLAMA17B_TOKEN_BASE64
  slack-bot-token: YOUR_SLACK_BOT_TOKEN_BASE64
  slack-team-id: YOUR_SLACK_TEAM_ID_BASE64
type: Opaque
